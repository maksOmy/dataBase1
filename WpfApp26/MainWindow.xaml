<Window x:Class="WpfApp26.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp26"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <Grid>
        <TextBox x:Name="TextBoxLogin" HorizontalAlignment="Left" Height="22" Margin="66,86,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="241" TextChanged="TextBoxLogin_TextChanged"/>
        <PasswordBox x:Name="TextBoxPass" HorizontalAlignment="Left" Margin="66,130,0,0" VerticalAlignment="Top" Width="241"/>
        <Label Content="         Log in" HorizontalAlignment="Left" Margin="126,29,0,0" VerticalAlignment="Top" Width="106"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="66,180,0,0" VerticalAlignment="Top" Width="75" Click="Button_Login_Click"/>
        <Button Content="Sign Up" HorizontalAlignment="Left" Margin="176,180,0,0" VerticalAlignment="Top" Width="131"/>
    </Grid>
</Window>
